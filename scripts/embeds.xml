    <Frame name="GatherLiteTracker" mixin="GatherLiteTracker">
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnUpdate>
                GatherLiteTracker:OnUpdate(elapsed)
            </OnUpdate>
        </Scripts>
    </Frame>

    <Button name="GatherLiteToggle" parent="WorldMapFrame" inherits="UIPanelButtonTemplate" text="Hide GatherLite">
        <Size>
            <AbsDimension x="140" y="20"/>
        </Size>
        <Anchors>
            <Anchor point="RIGHT" relativePoint="LEFT" relativeTo="WorldMapContinentDropDown">
                <Offset>
                    <AbsDimension x="0" y="0"/>
                </Offset>
            </Anchor>
        </Anchors>
        <Scripts>
            <OnLoad>
                self:SetFrameLevel(10);
            </OnLoad>
        </Scripts>
    </Button>

